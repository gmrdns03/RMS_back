<mapper namespace="com.project.LimeRMS.mapper.CommCdMapper">
    <select id="findCdNmByUserStat" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT cdNm FROM CommCd WHERE commCd = #{userStat}
