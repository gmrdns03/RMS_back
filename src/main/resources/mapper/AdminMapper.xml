<!-- AuthenticationMapper.xml -->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.LimeRMS.mapper.AdminMapper">
    <select id="findAllUser" resultType="com.project.LimeRMS.entity.User">
        SELECT * FROM User
    </select>
    <select id="findAuthIdByUserId" parameterType="java.lang.Integer" resultType="java.lang.Integer">
        SELECT authId FROM User WHERE userId = #{userId}
    </select>
</mapper>
