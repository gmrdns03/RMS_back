<!-- AuthenticationMapper.xml -->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.LimeRMS.Common.mapper.CommCdMapper">
    <select id="findCdNmByCd" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT cdNm FROM CommCd WHERE commCd = #{commCd}
    </select>
    <select id="findCommCdByHiCommCd" parameterType="java.lang.String" resultType="com.project.LimeRMS.User.dto.CommCdDto">
        SELECT * FROM CommCd WHERE hiCommCd = #{hiCommCd}
    </select>
</mapper>
